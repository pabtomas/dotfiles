{
  "id": "entrypoint",
  "path": {
    "entrypoint": {
      "sh": "{{ $all.path.opt.scripts }}/docker_entrypoint.sh",
      "d": "{{ $entrypoint.path.entrypoint.sh | strings.ReplaceAll \".sh\" \".d\" }}"
    },
    "check": {
      "sh": "{{ $entrypoint.path.entrypoint.d }}/98check.sh",
      "d": "{{ $entrypoint.path.check.sh | strings.ReplaceAll \".sh\" \".d\" }}"
    }
  }
}
